<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="ListviewHeaderImage">
        <attr name="headerImage" format="reference"/>
        <attr name="enabledHead" format="boolean"/>
    </declare-styleable>
    <!-- Toolbar -->
    <declare-styleable name="WJHToolbar">
        <attr name="tb_icon" format="reference" />
        <attr name="tb_circle_bg" format="color" />
        <attr name="tb_choose_color" format="color" />
        <attr name="tb_text" format="string" />
        <attr name="tb_textsize" format="dimension" />
        <attr name="tb_modo">
            <enum name="onaly_color" value="1" />
            <enum name="onaly_circle_bg" value="2"/>
            <enum name="both" value="3"></enum>
        </attr>
    </declare-styleable>
    <!-- Toolbar -->
    <declare-styleable name="NewView">
        <attr name="newview_bg" format="color" />
        <attr name="newview_text" format="string" />
        <attr name="newview_textsize" format="dimension" />
        <attr name="newview_textcolor" format="color" />
    </declare-styleable>
</resources>